@{
    ViewBag.Title = "ImportCountries";
}

<form asp-action="ImportCountries" asp-asp-controller="Person" method="post" enctype="multipart/form-data">
    <div class="w-50">
        <div class="flex w-100">
            <div class="w-25">
                <div class="form-label">Choose File</div>
            </div>
            <div class="flex-1">
                <div class="form-input">
                    <input type="file" name="cFile" />
                </div>
            </div>
        </div>

        <div class="flex w-100">
            <div class="w-25">
            </div>
            <div class="flex-1">
                <button type="submit" class="button button-blue-back">Upload</button>
            </div>
        </div>
    </div>
</form>
