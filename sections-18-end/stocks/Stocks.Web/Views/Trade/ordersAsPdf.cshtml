@{
    Layout = null;
    var link = "http://" + Context.Request.Host + "/stylesheet.css";
}
<link rel="stylesheet" href="@link" />

@model List<Order>

<h3>Orders</h3>
<table class="table">
    <thead>
        <tr>
            <th>Date and Time</th>
            <th>Stock</th>
            <th>Order Type</th>
            <th>Quantity</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (Order order in Model)
        {
            <tr>
                <td>@order.DateAndTimeOfOrder</td>
                <td>@order.StockInfo</td>
                <td>@order.OrderType</td>
                <td>@order.Quantity</td>
                <td>@order.Price</td>
            </tr>
        }
    </tbody>
</table>

